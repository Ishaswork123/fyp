<script>
  window.addEventListener("scroll", function() {
    var sliderContainer = document.querySelector(".home_slider_container");
    
    if (window.scrollY > 50) { // Adjust the value (50) based on the amount of scroll you want
        sliderContainer.classList.add("home_slider_minimized");
    } else {
        sliderContainer.classList.remove("home_slider_minimized");
    }
});

</script>

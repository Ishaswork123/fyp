    openLabModalBtn.addEventListener("click", function (event) {
      event.preventDefault();
      labModal.style.display = "flex"; // or "block" if not using flex
    });
  }

  if (closeLabModalBtn && labModal) {
    closeLabModalBtn.addEventListener("click", function () {
      labModal.style.display = "none";
    });
  }


function toggleSubmenu(event, id) {
        event.preventDefault(); // Prevent the default link action

        let submenu = document.getElementById(id);
        
        // Close other submenus when opening a new one
        document.querySelectorAll('.submenu').forEach(menu => {
            if (menu.id !== id) {
                menu.style.display = 'none';
            }
        });

        // Toggle the clicked submenu
        submenu.style.display = (submenu.style.display === "none" || submenu.style.display === "") ? "block" : "none";
    }

	document.getElementById("room").addEventListener("change", function() {

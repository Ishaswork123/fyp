	<div class="courses">
		<h2>Experiments</h2>
		<div class="row courses_row">
			<% experiments.forEach(function(exp, index) { %>
				<div class="col-lg-4 course_col">
					<div class="course" onclick="openModal('<%= index %>')">
						<div class="course_image">
							<img src="<%= exp.image %>" alt="<%= exp.title %>" />
						</div>
						<div class="course_body">
							<h3 class="course_title"> <%= exp.title %>
							</h3>
							
							<p><%= exp.description %></p>
						</div>
					</div>
				</div>
			
				<!-- Modal for Each Experiment -->
				<div id="expModal<%= index %>" class="modal">
					<div class="modal-content">
						<span class="close" onclick="closeModal('<%= index %>')">&times;</span>
						<h3><%= exp.title %></h3>
						<div class="modal-links">
							<a href="/guided/<%= exp.id %>" class="btn btn-primary">Guided Experiment</a>
							<a href="/exp/<%= exp.id %>" class="btn btn-secondary">Custom Experiment</a>
							<a href="/std/resource/<%= exp.exp_no%>" class="btn btn-info">Learning Material</a>
						</div>
					</div>
				</div>
			<% }) %>
			
		</div> <br>
		<div class="text-center">

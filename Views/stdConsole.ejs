function openModal(index) {
    const modal = document.getElementById("expModal" + index);
    if (modal) {
      modal.style.display = "flex"; // your CSS uses flex
    }
  }

  function closeModal(index) {
    const modal = document.getElementById("expModal" + index); // ✅ Corrected this line
    if (modal) {
      modal.style.display = "none";
    }
  }

  // Optional: Close modal when clicking outside of it
  window.onclick = function (event) {
    const modals = document.querySelectorAll(".modal");
    modals.forEach((modal) => {
      if (event.target === modal) {
        modal.style.display = "none";
      }
    });
  };

 // Close all modals on any key press
document.addEventListener("keydown", function () {
  const modals = document.querySelectorAll(".modal");
  modals.forEach((modal) => (modal.style.display = "none"));
});


  // Make openModal and closeModal globally available (optional)
  window.openModal = openModal;
  window.closeModal = closeModal;

